{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "lib": ["ES6", "ESNext"],
    "noEmit": true,
    "target": "ESNext",
    "skipLibCheck": true,
    "baseUrl": "src",
    "paths": {
      "~/*": ["*"]
    }
  },
  "exclude": ["node_modules", "babel.config.js", "metro.config.js"]
}
